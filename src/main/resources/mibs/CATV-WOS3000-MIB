-- *****************************************************************************
-- *                    CATV WOS3000 Module Transmitter (TypeID = 3)
-- *****************************************************************************
transTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TransEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "CATV WOS3000 Module Transmitter"
    ::= { slotDevices 3 }
    
transEntry OBJECT-TYPE
    SYNTAX TransEntry
    ACCESS not-accessible
    STATUS mandatory
    INDEX { transSlot }
    ::= { transTable 1 }

TransEntry ::= SEQUENCE
{
    transSlot               INTEGER,
    
    transOutPower           Unsigned32,
    transOutPowerAlarmHi    Unsigned32,
    transOutPowerAlarmLo    Unsigned32,
    
    transLaserBias          Unsigned32,
    transLaserBiasAlarmHi   Unsigned32,
    transLaserBiasAlarmLo   Unsigned32,
    
    transLaserTemp          Unsigned32,
    transLaserTempAlarmHi   Unsigned32,
    transLaserTempAlarmLo   Unsigned32,
    
    transLaserTec           INTEGER,
    transLaserTecAlarmHi    INTEGER,
    transLaserTecAlarmLo    INTEGER,
    
    transRFLevel            Unsigned32,
    transRFLevelAlarmHi     Unsigned32,
    transRFLevelAlarmLo     Unsigned32,
    
    transCurRFMode          Unsigned32,
    
    transAGCOffset          INTEGER,
    transAGCOffsetMin       INTEGER,
    transAGCOffsetMax       INTEGER,
    
    transMGCAtt             Unsigned32,
    transMGCAttMin          Unsigned32,
    transMGCAttMax          Unsigned32,
    
    transChanNum            Unsigned32,
    transChanNumMin         Unsigned32,
    transChanNumMax         Unsigned32,
    
    transLaserCtrl          Unsigned32,
    
    transCPUTemp            INTEGER,
    transSN                 DisplayString,
    transVersion            Unsigned32,
    transWTimer             Unsigned32
}

transSlot OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    ::= { transEntry 1 }

transOutPower OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Unit 0.1mW"
    ::= { transEntry 2 }
    
