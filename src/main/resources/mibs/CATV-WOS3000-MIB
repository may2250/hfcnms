    SYNTAX Unsigned32
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Unit 0.1dBuV"
    ::= { transEntry 15 }
    
transRFLevelAlarmLo OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Unit 0.1dBuV"
    ::= { transEntry 16 }
    
transCurRFMode OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "0 = AGC; !0 = MGC"
    ::= { transEntry 17 }
    
