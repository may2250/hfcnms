**********************************************************************
-- *                    CATV WOS3000 TRAP
-- *****************************************************************************
wos3kTrapRestart TRAP-TYPE
    ENTERPRISE wos3000Ident
    VARIABLES { wos3kPosixTimer, managerMacAddr }
    DESCRIPTION
        "WOS3000 Restart."
    ::= 1

wos3kTrapDeviceUp TRAP-TYPE
    ENTERPRISE wos3000Ident
    VARIABLES { wos3kPosixTimer, wos3kModuleAlarmSlot, wos3kModuleType }
    DESCRIPTION
        "WOS3000 Module Uplink."
    ::= 2

wos3kTrapDeviceDown TRAP-TYPE
    ENTERPRISE wos3000Ident
    VARIABLES { wos3kPosixTimer, wos3kModuleAlarmSlot, wos3kModuleType }
    DESCRIPTION
        "WOS3000 Module Downlink."
    ::= 3
    
wos3kTrapAlarm TRAP-TYPE
    ENTERPRISE wos3000Ident
    VARIABLES { wos3kPosixTimer, wos3kModuleAlarmSlot, wos3kModuleType, wos3kModuleAlarmText }
    DESCRIPTION
        "WOS3000 Module Alarm."
    ::= 4
    
-- *****************************************************************************
END