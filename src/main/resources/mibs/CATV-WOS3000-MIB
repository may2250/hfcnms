    SYNTAX Unsigned32
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Unit 1dB"
    ::= { transEntry 23 }
    
transChanNum OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-write
    STATUS mandatory
    ::= { transEntry 24 }
    
transChanNumMin OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-only
    STATUS mandatory
    ::= { transEntry 25 }
    
transChanNumMax OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-only
    STATUS mandatory
    ::= { transEntry 26 }
    
transLaserCtrl OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "bit7 = 1--with optical ATT TRANS moudle,bit7 = 0--NO otical ATT TRANS moudle,
         bit6-bit2,otical ATT Value,range:10-44,unit:0.1v
         bit0 = 0 LASER OFF; bit0 = 1 laser ON."
    ::= { transEntry 27 }

transCPUTemp OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Unit 0.1 Degree of Celsius."
    ::= { transEntry 28 }
    
transSN OBJECT-TYPE
    SYNTAX DisplayString
    ACCESS read-only
    STATUS mandatory
    ::= { transEntry 29 }
    
transVersion OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Devide by 100,e.g, 245 -> Version 2.45."
    ::= { transEntry 30 }
    
transWTimer OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Unit 0.5 hours per one tick."
    ::= { transEntry 31 }
    
-- *****************************************************************************
-- *                    CATV WOS3000 Module Receiver (TypeID = 4)
-- *****************************************************************************
recvTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RecvEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "CATV WOS3000 Module Receiver"
    ::= { slotDevices 4 }
    
recvEntry OBJECT-TYPE
    SYNTAX RecvEntry
    ACCESS not-accessible
    STATUS mandatory
    INDEX { recvSlot }
    ::= { recvTable 1 }
    
RecvEntry ::= SEQUENCE
{
    recvSlot                INTEGER,
    
    recvRecvPower           INTEGER,
    recvRecvPowerAlarmHi    INTEGER,
    recvRecvPowerAlarmLo    INTEGER,
    
    recvOutRFLevel          Unsigned32,
    recvOutRFLevelAlarmHi   Unsigned32,
    recvOutRFLevelAlarmLo   Unsigned32,
    
    recvOptAGCEn            Unsigned32,
    
    recvOptAGCOffset        INTEGER,
    recvOptAGCOffsetMin     INTEGER,
    recvOptAGCOffsetMax     INTEGER,
    
    recvAtt                 Unsigned32,
    recvAttMin              Unsigned32,
    recvAttMax              Unsigned32,
    
    recvEQ                  Unsigned32,
    recvEQMin               Unsigned32,
    recvEQMax               Unsigned32,
    
    recvChanNum             Unsigned32,
    recvChanNumMin          Unsigned32,
    recvChanNumMax          Unsigned32,
    
    recvCPUTemp             INTEGER,
    recvSN                  DisplayString,
    recvVersion             Unsigned32,
    recvWTimer              Unsigned32
}
    
recvSlot OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    ::= { recvEntry 1 }

recvRecvPower OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
