    SYNTAX Unsigned32
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Unit 0.1mW"
    ::= { transEntry 3 }
    
transOutPowerAlarmLo OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Unit 0.1mW"
    ::= { transEntry 4 }
    
