CATV-WOS3000-MIB DEFINITIONS ::= BEGIN
-- *****************************************************************************
-- *                    MIB Basic Structure Defines
-- *****************************************************************************

IMPORTS
    OBJECT-IDENTITY, MODULE-IDENTITY, OBJECT-TYPE, Unsigned32
        FROM SNMPv2-SMI
    IpAddress, Counter, Gauge, TimeTicks, enterprises
        FROM RFC1155-SMI
    DisplayString, PhysAddress
        FROM RFC1213-MIB
    TRAP-TYPE
        FROM RFC-1215;
    
xuyaoHongRoot   OBJECT IDENTIFIER ::= { enterprises 2000 }
prevailTree     OBJECT IDENTIFIER ::= { xuyaoHongRoot 1 }
wos3000Ident    OBJECT-IDENTITY
    STATUS  current
    DESCRIPTION
        " Defines the base OID for WOS3000, (Contact 13588838938). "
    ::= { prevailTree 3000 }
    
slotDevices     OBJECT IDENTIFIER ::= { wos3000Ident 1 }

-- *****************************************************************************
-- *                    CATV WOS3000 Frame Variables
-- *****************************************************************************
wos3kPosixTimer OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Get or set the posix time for WOS3000 system."
    ::= { wos3000Ident 2 }
    
wos3kAlarmLastIndex OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The last index of alarm log table."
    ::= { wos3000Ident 3 }

wos3kAlarmTable OBJECT-TYPE
    SYNTAX SEQUENCE OF Wos3kAlarmEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Alarm log table."
    ::= { wos3000Ident 4 }
    
wos3kAlarmEntry OBJECT-TYPE
    SYNTAX Wos3kAlarmEntry
    ACCESS not-accessible
    STATUS mandatory
    INDEX { wos3kAlarmIndex }
    ::= { wos3kAlarmTable 1 }
    
Wos3kAlarmEntry ::= SEQUENCE
{
    wos3kAlarmIndex         INTEGER,
    wos3kDeviceSlot         INTEGER,
    wos3kDeviceType         Unsigned32,
    wos3kAlarmText          DisplayString,
    wos3kAlarmPosixTime     Unsigned32
}
    
wos3kAlarmIndex OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Index of alarm log."
    ::= { wos3kAlarmEntry 1 }
    
wos3kDeviceSlot OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Slot number for alarm device."
    ::= { wos3kAlarmEntry 2 }
    
wos3kDeviceType OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Type of alarm device."
    ::= { wos3kAlarmEntry 3 }
    
wos3kAlarmText OBJECT-TYPE
    SYNTAX DisplayString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Text of alarm log."
    ::= { wos3kAlarmEntry 4 }
    
wos3kAlarmPosixTime OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Posix time of the alarm log item."
    ::= { wos3kAlarmEntry 5 }
    
wos3kTrapHostTable OBJECT-TYPE
    SYNTAX SEQUENCE OF Wos3kTrapHostEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Trap Host IP table."
    ::= { wos3000Ident 5 }
    
wos3kTrapHostEntry OBJECT-TYPE
    SYNTAX Wos3kTrapHostEntry
    ACCESS not-accessible
    STATUS mandatory
    INDEX { wos3kTrapHostIndex }
    ::= { wos3kTrapHostTable 1 }
    
Wos3kTrapHostEntry ::= SEQUENCE
{
    wos3kTrapHostIndex         INTEGER,
    wos3kTrapHostIPAddr        IpAddress,
}
    
wos3kTrapHostIndex OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Index of Trap HostIP table."
    ::= { wos3kTrapHostEntry 1 }
    
wos3kTrapHostIPAddr OBJECT-TYPE
    SYNTAX IpAddress
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "IP Address of Trap Host."
    ::= { wos3kTrapHostEntry 2 }
    
wos3kModuleAlarmSlot OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Used by Alarm Module."
    ::= { wos3000Ident 6 }
    
wos3kModuleAlarmText OBJECT-TYPE
    SYNTAX DisplayString
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Used by Alarm Module."
    ::= { wos3000Ident 7 }
    
wos3kModuleType OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Used by Alarm Module,
            1 - MANAGER;
            2 - POWER;
            3 - TRANSMITTER;
            4 - RECEIVER;
            5 - RETURN RECEIVER;
            6 - OPTICAL SWITCHER;
            7 - RF SWITCHER;
            9 - EDFA;
            10 - DOUBLE RECEIVER"
    ::= { wos3000Ident 8 }
    
-- *****************************************************************************
-- *                    CATV WOS3000 Module Manager (TypeID = 1)
-- *****************************************************************************
managerTable OBJECT-TYPE
    SYNTAX SEQUENCE OF ManagerEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "CATV WOS3000 Module Manager"
    ::= { slotDevices 1 }
    
managerEntry OBJECT-TYPE
    SYNTAX ManagerEntry
    ACCESS not-accessible
    STATUS mandatory
    INDEX { managerSlot }
    ::= { managerTable 1 }

ManagerEntry ::= SEQUENCE
{
    managerSlot             INTEGER,
    managerFanCtrl          OCTET STRING,
    managerIPAddr           IpAddress,
    managerGateway          IpAddress,
    managerNetmask          IpAddress,
    managerMacAddr          PhysAddress,
    managerCPUTemp          INTEGER,
    managerSN               DisplayString,
    managerVersion          Unsigned32,
    managerWTimer           Unsigned32
}

managerSlot OBJECT-TYPE
    SYNTAX INTEGER (11)
    ACCESS read-only
    STATUS mandatory
    ::= { managerEntry 1 }

managerFanCtrl OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(5))
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Fan status of wos3000, byte value is 0, the fan is ON; 
            else the fan is OFF."
    ::= { managerEntry 2 }

managerIPAddr OBJECT-TYPE
    SYNTAX IpAddress
    ACCESS read-only
    STATUS mandatory
    ::= { managerEntry 3 }
    
managerGateway OBJECT-TYPE
    SYNTAX IpAddress
    ACCESS read-only
    STATUS mandatory
    ::= { managerEntry 4 }
    
managerNetmask OBJECT-TYPE
    SYNTAX IpAddress
    ACCESS read-only
    STATUS mandatory
    ::= { managerEntry 5 }
    
managerMacAddr OBJECT-TYPE
    SYNTAX PhysAddress
    ACCESS read-only
    STATUS mandatory
    ::= { managerEntry 6 }
    
managerCPUTemp OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Unit 0.1 Degree of Celsius."
    ::= { managerEntry 7 }
    
managerSN OBJECT-TYPE
    SYNTAX DisplayString
    ACCESS read-only
    STATUS mandatory
    ::= { managerEntry 8 }
    
managerVersion OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Devide by 100,e.g, 245 -> Version 2.45."
    ::= { managerEntry 9 }
    
managerWTimer OBJECT-TYPE
    SYNTAX Unsigned32
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Unit 0.5 hours per one tick."
    ::= { managerEntry 10 }
    
-- *****************************************************************************
-- *                    CATV WOS3000 Module Power (TypeID = 2)
-- *****************************************************************************
powerTable OBJECT-TYPE
    SYNTAX SEQUENCE OF PowerEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "CATV WOS3000 Module Power"
    ::= { slotDevices 2 }
    
powerEntry OBJECT-TYPE
    SYNTAX PowerEntry
    ACCESS not-accessible
    STATUS mandatory
    INDEX { powerSlot }
    ::= { powerTable 1 }

PowerEntry ::= SEQUENCE
{
    powerSlot               INTEGER,
    
    powerP5VTest            INTEGER,
    powerP5VTestAlarmHi     INTEGER,
    powerP5VTestAlarmLo     INTEGER,
    
    powerM5VTest            INTEGER,
    powerM5VTestAlarmHi     INTEGER,
    powerM5VTestAlarmLo     INTEGER,
    
    powerP24VTest           INTEGER,
    powerP24VTestAlarmHi    INTEGER,
    powerP24VTestAlarmLo    INTEGER,
    
    powerFanOnTemp          INTEGER,
    powerFanOnTempMin       INTEGER,
    powerFanOnTempMax       INTEGER,
    
    powerCPUTemp            INTEGER,
    powerSN                 DisplayString,
    powerVersion            Unsigned32,
    powerWTimer             Unsigned32
}

