-- *****************************************************************************
-- *                    CATV WOS3000 Module Transmitter (TypeID = 3)
-- *****************************************************************************
transproTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TransproEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "CATV WOS3000 Module Pro-Transmitter"
    ::= { slotDevices 13 }
    
transproEntry OBJECT-TYPE
    SYNTAX TransproEntry
    ACCESS not-accessible
    STATUS mandatory
    INDEX { transproSlot }
    ::= { transproTable 1 }

TransproEntry ::= SEQUENCE
{
    transproSlot               INTEGER,
    
    transproOutPower           Unsigned32,
    transproOutPowerAlarmHi    Unsigned32,
    transproOutPowerAlarmLo    Unsigned32,

    transproOutPowerafteratt          INTEGER,
    transproOutPowerafterattAlarmHi    INTEGER,
    transproOutPowerafterattAlarmLo    INTEGER,

    transproInputPower           INTEGER,
    transproInputPowerAlarmHi    INTEGER,
    transproInputPowerAlarmLo    INTEGER,
    
    transproLaserBias          Unsigned32,
    transproLaserBiasAlarmHi   Unsigned32,
    transproLaserBiasAlarmLo   Unsigned32,
    
    transproLaserTemp          Unsigned32,
    transproLaserTempAlarmHi   Unsigned32,
    transproLaserTempAlarmLo   Unsigned32,
    
    transproLaserTec           INTEGER,
    transproLaserTecAlarmHi    INTEGER,
    transproLaserTecAlarmLo    INTEGER,
    
    transproRFLevel            Unsigned32,
    transproRFLevelAlarmHi     Unsigned32,
    transproRFLevelAlarmLo     Unsigned32,

    transproOpAttCtrlMode          Unsigned32,
    
    transproOpAttValue          Unsigned32,
    transproOpAttValueMin       Unsigned32,
    transproOpAttValueMax       Unsigned32,
    
    transproOtoIPowerDef             Unsigned32,
    transproOtoIPowerDefMin          Unsigned32,
    transproOtoIPowerDefMax          Unsigned32,

    
    transproCurRFMode          Unsigned32,
    
    transproAGCOffset          INTEGER,
    transproAGCOffsetMin       INTEGER,
    transproAGCOffsetMax       INTEGER,
    
    transproMGCAtt             Unsigned32,
    transproMGCAttMin          Unsigned32,
    transproMGCAttMax          Unsigned32,
    
    transproChanNum            Unsigned32,
    transproChanNumMin         Unsigned32,
    transproChanNumMax         Unsigned32,
    
    transproLaserCtrl          Unsigned32,
    
    transproCPUTemp            INTEGER,
    transproSN                 DisplayString,
    transproVersion            Unsigned32,
    transproWTimer             Unsigned32
}

