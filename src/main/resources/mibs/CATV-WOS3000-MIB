-- *****************************************************************************
-- *                    CATV WOS3000 Module Retrurn-Receiver (TypeID = 5)
-- *****************************************************************************
retRecvTable OBJECT-TYPE
    SYNTAX SEQUENCE OF RetRecvEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "CATV WOS3000 Module Retrurn-Receiver"
    ::= { slotDevices 5 }
    
retRecvEntry OBJECT-TYPE
    SYNTAX RetRecvEntry
    ACCESS not-accessible
    STATUS mandatory
    INDEX { retRecvSlot }
    ::= { retRecvTable 1 }
    
RetRecvEntry ::= SEQUENCE
{
    retRecvSlot                 INTEGER,
    
    retRecvARecvPower           INTEGER,
    retRecvARecvPowerAlarmHi    INTEGER,
